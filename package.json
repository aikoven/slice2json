{
  "name": "slice2json",
  "version": "0.1.5",
  "description": "Parser for the Slice language",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && tsc -P tsconfig.build.json && cpx \"src/*.ohm\" lib",
    "prepare": "npm test && npm run build"
  },
  "files": [
    "src",
    "lib"
  ],
  "repository": "aikoven/slice2json",
  "author": "Daniel Lytkin <dan.lytkin@gmail.com>",
  "license": "GPL-2.0",
  "dependencies": {
    "ohm-js": "^0.14.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "cpx": "^1.5.0",
    "glob": "^7.1.4",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "slice2js": "~3.7.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
